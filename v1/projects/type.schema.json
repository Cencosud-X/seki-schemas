{
  "$schema": "http://json-schema.org/schema",
  "required": [
    "name",
    "description",
    "path",
    "kind",
    "settings"
  ],
  "properties": {
    "name": {
      "type": "string",
      "description": "Project name",
      "minLength": 2,
      "readOnly": true
    },
    "description": {
      "type": "string",
      "description": "Project description",
      "minLength": 2,
      "readOnly": true
    },
    "path": {
      "type": "string",
      "description": "Project relative path to install",
      "minLength": 2,
      "readOnly": true
    }
  }
}
